{
  "name": "tagfortag-frontend",
  "description": "TODO: Write tagfortag-frontend description",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "author": "Michael Schwobe <mschwobe@gmail.com> (http://michaelschwobe.com)",
  "repository": "github:michaelschwobe/tagfortag-frontend",
  "scripts": {
    "lint": "eslint .",
    "test": "react-scripts test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "formik": "^1.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-test-renderer": "^16.7.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^0.0.0",
    "prettier": "^1.15.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "import",
      "jsx-a11y",
      "react",
      "react-hooks",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "all"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react/jsx-filename-extension": "off",
      "react/no-children-prop": "off"
    },
    "overrides": [
      {
        "files": [
          "setupTests.js"
        ],
        "rules": {
          "import/no-extraneous-dependencies": "off"
        }
      },
      {
        "files": [
          "*.stories.js",
          "*.test.js"
        ],
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "react/prop-types": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "/coverage",
    "/build",
    "/public"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
